class Solution(object):
    def findClosestNumber(self, nums):
         dif=abs(nums[0])-0
         for i in nums:
             if abs(i)-0 <dif:
                dif=abs(i)-0
         for i in nums:
             if abs(i)==dif:
                 if abs(i) not in nums:
                     return i
                 elif abs(i) in nums:
                     return abs(i)
